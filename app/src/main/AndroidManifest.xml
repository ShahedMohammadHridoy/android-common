<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.AndroidComponents"
        tools:targetApi="31">
        <activity
            android:name=".SpinnerActivity"
            android:exported="false"
            android:label="@string/spinner"
            android:parentActivityName=".RootContainersActivity" />
        <activity
            android:name=".RootContainersActivity"
            android:exported="false"
            android:label="@string/containers"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".arraytojson.RootArrayToJsonActivity"
            android:exported="false" />
        <activity
            android:name=".ButtonsCheckBoxActivity"
            android:exported="false"
            android:label="@string/checkbox"
            android:parentActivityName=".RootButtonActivity" />
        <activity
            android:name=".ButtonsImageButtonActivity"
            android:exported="false"
            android:label="@string/image_button"
            android:parentActivityName=".RootButtonActivity" />
        <activity
            android:name=".ButtonsSwitchActivity"
            android:exported="false"
            android:label="Switch"
            android:parentActivityName=".RootButtonActivity" />
        <activity
            android:name=".RootButtonActivity"
            android:exported="false"
            android:label="Buttons"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".RootTextActivity"
            android:exported="false"
            android:label="Text"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".RootCommonActivity"
            android:exported="false"
            android:label="Common"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".ImageViewActivity"
            android:exported="false"
            android:parentActivityName=".RootCommonActivity" />
        <activity
            android:name=".ButtonsButtonActivity"
            android:exported="false"
            android:label="@string/button"
            android:parentActivityName=".RootCommonActivity" />
        <activity
            android:name=".TextViewActivity"
            android:exported="false"
            android:label="@string/textview"
            android:parentActivityName=".RootCommonActivity" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>